<%= form_for [@project, @entry] do |f| %>
	
	<%= f.label :hours %>
	<%= f.text_field :hours %>
	<br>
	<br>
	<%= f.label :minutes %>
	<%= f.text_field :minutes %>
	<br>
	<br>
	<%= f.label :date %>
	<%= f.date_field :date %>
	<br>
	<br>
	<%= f.label :comments %>
	<%= f.text_area :comments %>

	<%= f.submit 'save' %>

<% end %>

<%= link_to 'Back', project_entry_path %>